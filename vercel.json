{
  "rewrites": [
    {
      "source": "/causes",
      "destination": "/causes.html"
    },
    {
      "source": "/causes/",
      "destination": "/causes.html"
    }
  ],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    }
  ]
}